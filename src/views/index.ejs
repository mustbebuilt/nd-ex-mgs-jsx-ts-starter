<%- include('partials/head') %>

    <div class="container">
      
      <h1>Sheffield Streaming</h1>

            <section class="home">
        <% films.forEach(function(film){ %>

        <div class="filmThumb">
          <div>
            <a href="/film/<%= film._id %>"
              ><img
                src="/images/<%= film.filmImage %>"
                alt="<%= film.filmTitle %>"
            /></a>
          </div>
          <h3>
            <a href="/film/<%= film._id %>"><%= film.filmTitle %></a>
          </h3>
        </div>
        <% }) %>
      </section>
 
    </div>
<%- include('partials/footer') %>
  </body>
</html>
